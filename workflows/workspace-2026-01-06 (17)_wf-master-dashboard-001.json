      "typeVersion": 1,
      "position": [
        300,
        350
      ]
    },
    {
      "parameters": {
        "url": "https://api.misp-project.org/rest/events",
        "authentication": "genericCredentialType",
        "genericAuthType": "mispApi",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Authorization",
              "value": "={{$node['misp-credentials'].json['api_key']}}"
