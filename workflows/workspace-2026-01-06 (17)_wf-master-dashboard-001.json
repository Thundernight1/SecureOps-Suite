          "priority": {
            "name": "Medium"
          }
        }
      },
      "id": "weekly-report",
      "name": "Create Weekly Report",
      "type": "n8n-nodes-base.jira",
      "typeVersion": 1,
      "position": [
        700,
        350
      ],
      "credentials": {
